{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "sscript",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "scene"
          }
        ]
      }
    },
    "scene": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "slug"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "block"
              },
              {
                "type": "REPEAT",
                "content": {
                  "type": "STRING",
                  "value": "\n"
                }
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "transition"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "STRING",
            "value": "\n"
          }
        }
      ]
    },
    "block": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "dialogue"
        },
        {
          "type": "SYMBOL",
          "name": "action"
        }
      ]
    },
    "slug": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "int_ext_indicator"
        },
        {
          "type": "STRING",
          "value": " "
        },
        {
          "type": "SYMBOL",
          "name": "location"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": " - "
                },
                {
                  "type": "SYMBOL",
                  "name": "additional_specifier"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": " - "
        },
        {
          "type": "SYMBOL",
          "name": "time"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "transition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "transition_name"
        },
        {
          "type": "STRING",
          "value": " TO:"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "action": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "line"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "dialogue": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "name"
        },
        {
          "type": "STRING",
          "value": "\n"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "dialogue_paranthesis"
                },
                {
                  "type": "STRING",
                  "value": "\n"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "dialogue_line"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "name": {
      "type": "PATTERN",
      "value": "[A-Z][A-Z \\.']*[A-Z]"
    },
    "dialogue_paranthesis": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "dialogue_line": {
      "type": "PATTERN",
      "value": "[^(\\n][^\\n]*[^)\\n]"
    },
    "string": {
      "type": "PATTERN",
      "value": "\"[^\"]*\""
    },
    "line": {
      "type": "PATTERN",
      "value": "[^\\n:][^\\n]*[^\\n:]"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-z]+"
    },
    "int_ext_indicator": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "INT."
        },
        {
          "type": "STRING",
          "value": "EXT."
        },
        {
          "type": "STRING",
          "value": "INT./EXT."
        }
      ]
    },
    "location": {
      "type": "SYMBOL",
      "name": "name"
    },
    "time": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "DAY"
        },
        {
          "type": "STRING",
          "value": "NIGHT"
        },
        {
          "type": "STRING",
          "value": "MORNING"
        },
        {
          "type": "STRING",
          "value": "AFTERNOON"
        },
        {
          "type": "STRING",
          "value": "EVENING"
        },
        {
          "type": "STRING",
          "value": "DUSK"
        },
        {
          "type": "STRING",
          "value": "DAWN"
        },
        {
          "type": "STRING",
          "value": "LATE NIGHT"
        },
        {
          "type": "STRING",
          "value": "MIDNIGHT"
        },
        {
          "type": "STRING",
          "value": "NOON"
        }
      ]
    },
    "additional_specifier": {
      "type": "SYMBOL",
      "name": "name"
    },
    "transition_name": {
      "type": "SYMBOL",
      "name": "name"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}
        {
          "type": "REPEAT",
          "content": {
            "type": "STRING",
            "value": "\n"
          }
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "block"
              },
              {
                "type": "REPEAT",
                "content": {
                  "type": "STRING",
                  "value": "\n"
                }
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "transition"
        }
      ]
    },
    "block": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "dialogue"
        },
        {
          "type": "SYMBOL",
          "name": "action"
        }
      ]
    },
    "slug": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ":: "
        },
        {
          "type": "SYMBOL",
          "name": "int_ext_indicator"
        },
        {
          "type": "STRING",
          "value": " "
        },
        {
          "type": "SYMBOL",
          "name": "slug_location"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": " - "
                },
                {
                  "type": "SYMBOL",
                  "name": "additional_specifier"
                }

            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": " - "
        },
        {
          "type": "SYMBOL",
          "name": "time"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "transition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ":: "
        },
        {
          "type": "SYMBOL",
          "name": "transition_name"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "action": {
      "type": "SEQ",
      "members": [

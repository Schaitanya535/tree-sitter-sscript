    "dialogue": {
      "type": "SEQ",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "character_name"
            },
            {
              "type": "STRING",
              "value": "\n"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "dialogue_line"
          }
        },
        {
          "type": "STRING",
          "value": "\n\n"
        }
      ]
    },
    "character_name": {
      "type": "SYMBOL",
      "name": "name"
    },
    "name": {
      "type": "PATTERN",
      "value": "[A-Z][A-Z \\.'(]*[A-Z)]"
    },
    "dialogue_type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "O.S."
        },
        {
          "type": "STRING",
          "value": "CONT'D"
        },
        {
          "type": "STRING",
          "value": "MORE"
        }
      ]
    },
    "dialogue_paranthesis": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "dialogue_line": {
      "type": "PATTERN",
      "value": "[^\\n]*"
    },
    "string": {
      "type": "PATTERN",
      "value": "\"[^\"]*\""
    },
    "line": {
      "type": "PATTERN",
      "value": "[^\\n:][^\\n]*[^\\n:]"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-z]+"
    },
    "int_ext_indicator": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "INT."
        },
        {
          "type": "STRING",
          "value": "EXT."
        },
        {
          "type": "STRING",
          "value": "INT./EXT."
        }
      ]
    },
    "slug_location": {
      "type": "SYMBOL",
      "name": "name"
    },
    "time": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "DAY"
        },
        {
          "type": "STRING",
          "value": "NIGHT"
        },
        {
          "type": "STRING",
          "value": "MORNING"
        },
        {
          "type": "STRING",
          "value": "AFTERNOON"
        },
        {
          "type": "STRING",
          "value": "EVENING"
        },
        {
          "type": "STRING",
          "value": "DUSK"
        },
        {
          "type": "STRING",
          "value": "DAWN"
        },
        {
          "type": "STRING",
          "value": "LATE NIGHT"
        },
        {
          "type": "STRING",
          "value": "MIDNIGHT"
        },
        {
          "type": "STRING",
          "value": "NOON"
        }
      ]
    },
    "additional_specifier": {
      "type": "SYMBOL",
      "name": "name"
    },
    "transition_name": {
      "type": "SYMBOL",
      "name": "name"
    },
    "extras": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\s"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        }
      ]
    },
    "comment": {
      "type": "PATTERN",
      "value": "#.*"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
